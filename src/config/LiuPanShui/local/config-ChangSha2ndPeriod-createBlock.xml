<?xml version="1.0" ?>
<!-- 新增拥堵点页面的地图 -->
<configuration>
	<style>
		<!--textColor,backgroundColor,rolloverColor,selectionColor,titleColor -->
		<colors>0x000000,0xFFFFFF,0xDDDDDD,0x3E6FBC,0x3E6FBC</colors>
		<navigationcolors>0xFFFFFF,0x3E6FBC,0xDDDDDD,0x3E6FBC,0xFFFFFF</navigationcolors>
		<alpha>0.8</alpha>
		<font name="微软雅黑" size="14" />
	</style>

	<arcgisserverip>128.64.151.202:6080</arcgisserverip>

	<geometryservice
		url="http://{serverip}/arcgis/rest/services/Utilities/Geometry/GeometryServer" />

	<!-- UI elements -->
	<widget left="10" top="10" config="widgets/Navigation/NavigationWidget.xml"
		url="widgets/Navigation/NavigationWidget.swf" />

	<map top="0" initialextent="104.799 26.573 104.878 26.615"
		scalebarvisible="false" esrilogovisible="false"
		doubleclickzoomenabled="false">
		<basemaps>
			<layer label="基础图层" type="tiled" visible="true"
				url="http://{serverip}/arcgis/rest/services/LiuPanShui/lps_base/MapServer" />
		</basemaps>
		<operationallayers>
			<layer label="发布段" type="dynamic" visible="true" autorefresh="30"
				   url="http://{serverip}/arcgis/rest/services/LiuPanShui/lps_fbd/MapServer">
				<sublayer id="1" popupconfig="popups/LiuPanShui/Popup-fbd-createBlock.xml"/>
				<sublayer id="3" popupconfig="popups/LiuPanShui/Popup-fbd-createBlock.xml"/>
				<sublayer id="5" popupconfig="popups/LiuPanShui/Popup-fbd-createBlock.xml"/>
				<sublayer id="7" popupconfig="popups/LiuPanShui/Popup-fbd-createBlock.xml"/>
				<sublayer id="9" popupconfig="popups/LiuPanShui/Popup-fbd-createBlock.xml"/>
			</layer>
		</operationallayers>
	</map>

	<!-- widgets organized into widget containers that manage close/open etc -->
	<!-- supported layout property options: horizontal(default)|float|vertical|fix -->
	<widgetcontainer layout="fix">
		<!-- <widget label="ControlPanel" preload="open" -->
		<!-- right="20" bottom="20" -->
		<!-- config="widgets/ControlPanel/ChangSha/IconControlPanelWidget-ChangSha2ndPeriod-test.xml" -->
		<!-- url="widgets/ControlPanel/IconControlPanelWidget.swf"/> -->

		<widget label="BlockOverlay" preload="open"
			config="widgets/Overlay/LiuPanShui/PointOverlayWidget-createBlock.xml"
			url="widgets/Overlay/PointOverlayWidget.swf" />

		<widget label="SendBlockCoordinate" preload="open"
			config="widgets/SendMapClickCoordinate/LiuPanShui/SendMapClickCoordinate-block.xml"
			url="widgets/SendMapClickCoordinate/SendMapClickCoordinateWidget.swf" />
	</widgetcontainer>
</configuration>